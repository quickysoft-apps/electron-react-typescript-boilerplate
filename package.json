    "typeface-roboto": "0.0.31",
    "typescript-collections": "^1.2.3",
    "utf-8-validate": "^3.0.3"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "false": {}
}
